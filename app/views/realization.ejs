<%- include('partials/header') %>
<main class="realization">
    <div class="grid grid-2">
        <% for(const image of realization.images){ %> 
            <div class="grid__items">
                <a class="grid__link" href="/">
                    <div class="grid__titleContainer"></div>
                    <img class="grid__img" src="<%= image.link %> " alt="<%= image.title %> ">
                </a>
            </div>
        <% } %> 
        <%# Image de cloture %> 
        <div class="grid__items">
            <a class="grid__link" href="/">
                <div class="grid__titleContainer"></div>
                <img class="grid__img" src="/img/realizations/LOGO.jpg" alt="">
            </a>
        </div>
    </div>
</main>
<%- include('partials/footer') %>